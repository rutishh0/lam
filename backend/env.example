# MongoDB Configuration
MONGO_URL=mongodb://localhost:27017
DB_NAME=university_agent_db

# Encryption Configuration
ENCRYPTION_MASTER_KEY=your-encryption-key-here

# Email Configuration (Gmail example)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
FROM_EMAIL=your-email@gmail.com

# Twilio Configuration (for SMS)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_FROM_NUMBER=+1234567890

# Frontend URL
FRONTEND_URL=http://localhost:3000

# API Keys for CAPTCHA Solving (optional)
TWO_CAPTCHA_API_KEY=your-2captcha-key
ANTI_CAPTCHA_API_KEY=your-anticaptcha-key

# Proxy Configuration (optional)
PROXY_POOL=proxy1:port,proxy2:port,proxy3:port

# Application Settings
LOG_LEVEL=INFO
MAX_CONCURRENT_BROWSERS=3
BROWSER_TIMEOUT=30000
DAILY_CHECK_TIME=09:00

# Security Settings
JWT_SECRET=your-jwt-secret-key
SESSION_SECRET=your-session-secret 